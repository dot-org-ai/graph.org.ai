{
  "name": "mdxdb",
  "version": "0.0.1",
  "description": "Database interface for MDX files",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "mdxdb": "./dist/cli.js"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./fs": {
      "import": "./dist/fs/index.js",
      "types": "./dist/fs/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run"
  },
  "keywords": [
    "mdx",
    "database",
    "fs"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^19.2.6",
    "typescript": "^5.0.0",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@mdxui/markdown": "file:../@mdxui/markdown",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "chokidar": "^4.0.3",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "glob": "^10.0.0",
    "lodash-es": "^4.17.21",
    "mdxld": "file:../mdxld",
    "papaparse": "^5.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sift": "^17.1.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  }
}
